{
  "name": "api-worker",
  "version": "1.0.7",
  "description": "A prototype ui for my personal projects",
  "author": "leonardo-amaral, yLost",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "npm run lint & npm run format & tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yLost/ylost-ui.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yLost/ylost-ui/issues"
  },
  "homepage": "https://github.com/yLost/ylost-ui#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
